import turtle
t=turtle.Turtle()
t.forward(100)
t.backward(40)
t.begin_fill()
t.left(90)
t.forward(80)
t.left(60)
t.forward(30)
t.left(60)
t.forward(30)
t.left(60)
t.forward(80)

t.fillcolor("blue")
#draw your shape here
t.end_fill()
t.penup()
t.goto(-50,0)
t.pendown()
t.right(180)
t.forward(80)
t.left(60)
t.forward(30)
t.left(60)
t.forward(30)
t.left(60)
t.forward(80)
t.penup()
t.goto(0,50)
t.pendown()
t.right(90)

t.forward(50)

turtle.bgcolor("blue")
turtle.done()
